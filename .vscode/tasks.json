{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "Ben's Pre Launch Tasks",
            "command": "/usr/bin/gcc",
            "args": [
                "-fcolor-diagnostics",
                "-fansi-escape-codes",
                "-g",
                "-lm",
                "-I${fileDirname}/include",
                "${fileDirname}/*.c",
                "-o",
                "${fileDirname}/obj/${fileBasenameNoExtension}.o"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": "build",
            "detail": "Task generated by Debugger."
        }
    ],
    "version": "2.0.0"
}